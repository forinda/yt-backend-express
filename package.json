{
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.10",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.11.0",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.15",
    "typescript": "^5.0.2",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "vitest": "^0.29.7"
  },
  "name": "youtube-backend-express",
  "version": "1.0.0",
  "description": "Simple backend API",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest dev",
    "test:coverage": "vitest --coverage run",
    "test:coverage:dev": "vitest --coverage dev",
    "test:ui": "vitest --ui",
    "dev:build": "ttsc -p tsconfig.json",
    "dev:watch": "ttsc -p tsconfig.json --watch",
    "dev:run": "nodemon",
    "dev": "concurrently \"npm run dev:watch\" \"npm run dev:run\"",
    "build": "pnpm clean && ttsc -p tsconfig.json",
    "clean": "rimraf dist",
    "build:prod": "ttsc -p tsconfig.json --build",
    "start": "node dist/src/index.js",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:migrate": "prisma migrate dev --name init"
  },
